{
  "name": "fuegojs",
  "description": "Opinionated static site generation framework built on React, Esbuild, and AWS",
  "version": "0.37.3",
  "license": "MIT",
  "main": "index.js",
  "types": "index.d.ts",
  "repository": "dvargas92495/fuegojs",
  "scripts": {
    "format": "prettier --write \"src/**/*\"",
    "lint": "eslint . --ext .ts,.tsx",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push origin main && git push --tags",
    "prebuild": "npm run lint",
    "build": "tsc",
    "test": "jest"
  },
  "peerDependencies": {
    "@cdktf/provider-aws": "9.0.7",
    "@cdktf/provider-github": "^2.0.7",
    "@dvargas92495/aws-clerk": "^0.1.0",
    "@dvargas92495/aws-email": "^0.1.1",
    "@dvargas92495/aws-serverless-backend": "^2.5.5",
    "@dvargas92495/aws-websocket": "^1.2.2",
    "@remix-run/dev": ">=1 <1.7.0",
    "@remix-run/react": ">=1 <1.7.0",
    "@remix-run/serve": ">=1 <1.7.0",
    "@types/aws-lambda": ">=8.10.83",
    "@types/node": ">=16",
    "@types/react": ">=17",
    "@types/react-dom": ">=17",
    "@types/ws": "^7.4.7",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "cdktf-cli": "^0.12.0",
    "concurrently": "6.5.1",
    "eslint": "^8.x",
    "mysql2": "^2.3.3",
    "prettier": "^2.3.2",
    "react": ">=17",
    "react-dom": ">=17",
    "ts-node": "^10.8.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-react-hooks": "2.2.2",
    "typescript": ">=4",
    "zod": "^3.17.10"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/archiver": "^5.1.1",
    "@types/express": "^4.17.13",
    "@types/git-repo-name": "^1.0.1",
    "@types/jest": "^27.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/pluralize": "0.0.29",
    "@types/rimraf": "^3.0.2",
    "@types/uuid": "^8.3.4",
    "esbuild-jest": "^0.5.0",
    "jest": "^27.0.6"
  },
  "dependencies": {
    "archiver": "^5.3.0",
    "aws-sdk": "^2.975.0",
    "axios": "^0.27.2",
    "change-case": "^4.1.2",
    "chokidar": "^3.5.2",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.10",
    "express": "^4.17.1",
    "jszip": "^3.7.1",
    "mime-types": "^2.1.32",
    "ngrok": "^4.2.2",
    "pluralize": "^8.0.0",
    "rimraf": "^3.0.2",
    "uuid": "^8.3.2"
  },
  "bin": {
    "fuego": "./cli.js"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "esbuild-jest"
    },
    "testRegex": "/tests/.*\\.test\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
